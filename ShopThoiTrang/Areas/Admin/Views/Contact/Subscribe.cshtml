@model IEnumerable<ShopThoiTrang.Models.Contact>

@{
    ViewBag.Title = "Đăng ký";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}
@section scripts {
    <!-- Bootstrap core JavaScript-->
    <script src="~/Public/admin/vendor/jquery/jquery.min.js"></script>
    <script src="~/Public/admin/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

    <!-- Core plugin JavaScript-->
    <script src="~/Public/admin/vendor/jquery-easing/jquery.easing.min.js"></script>

    <!-- Custom scripts for all pages-->
    <script src="~/Public/admin/js/sb-admin-2.min.js"></script>
}
<div class="card admin__scroll">
    <div class="card-header">
        <div class="row">
            <div class="col-md-6">
                <strong class="text-uppercase text-danger">Đăng ký</strong>
            </div>
        </div>
    </div>
    <div class="card-body p-3">
        <ul class="list-group">
            @foreach (var item in Model)
            {
                <li class="list-group-item">
                    <div class="card">
                        <div class="card-body">
                            <h5 class="card-title">@item.Email</h5>
                            <p class="card-text text-truncate">@item.Title</p>
                        </div>
                    </div>
                </li>
            }
        </ul>
    </div>
</div>
